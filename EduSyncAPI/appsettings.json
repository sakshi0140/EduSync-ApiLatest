{
  "ConnectionStrings": {
    "DefaultConnection": "<YOUR_DATABASE_CONNECTION_STRING>",
    "AzureBlobStorage": "<YOUR_BLOB_STORAGE_CONNECTION_STRING>"
  },

  "EventHubConfig": {
    "ConnectionString": "<YOUR_EVENT_HUB_CONNECTION_STRING>",
    "HubName": "<YOUR_EVENT_HUB_NAME>"
  },

  "Jwt": {
    "Key": "<YOUR_JWT_SECRET_KEY>",
    "Issuer": "EduSyncAPI",
    "Audience": "EduSyncUsers",
    "DurationInMinutes": 60
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*"
}
